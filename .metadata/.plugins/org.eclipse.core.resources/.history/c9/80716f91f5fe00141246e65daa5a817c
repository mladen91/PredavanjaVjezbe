package ba.BITCamp.Mladen.MojPaket;

public class MojaKlasa {

	public static void main(String[] args) {

		int h=4;
		int R=6;
		double r=R/2.0;
		
		double P=(2*r*r*Math.PI)+(2*r*Math.PI*h);
		double V=R*R*Math.PI*h;
		
		
		
		System.out.println("Povrsina valjka iznosi:"+P);
		System.out.println("Zapremina valjka iznosi:"+V);
	}

}
