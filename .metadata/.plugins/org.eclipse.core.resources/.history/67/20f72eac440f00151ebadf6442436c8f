package Predavanja;

public class Array2D {

	public static void print2DArray (int[][] array) {
		
		int[][] newArray =  new int[4][5];
		
		for (int i=0;i<newArray.length;i++) {
			for (int j=0;j<newArray[i].length;j++) {
				System.out.println((newArray[i][j]+" "));
			}
			
		}
	}
	
