package ba.vjezbe;

public class Main {

	public static void main(String[] args) {
		
		Singer s1 = new Singer("Dino Merlin", false, 1980, "Pop");
		System.out.println("Artist name: "+ s1.name);
		System.out.println("Is it a band: "+s1.isBand);
		System.out.println("When did he start do sing: "+s1.activationYear);
		System.out.println("Music genre: "+s1.genre);

	}

}
