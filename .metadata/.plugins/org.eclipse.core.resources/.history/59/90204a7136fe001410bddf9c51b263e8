		int a;
		
		if (a>5)  
		{
			System.out.println("a");
		}
		
		System.out.println("            ***********************");
		System.out.println("          *  ************************");
		System.out.println("         *    ************************");
		System.out.println("        *      ************************");
		System.out.println("       *    	************************");
		System.out.println("      *          ************************");
		System.out.println("     *            ************************");
		System.out.println("    ***************************************");
		System.out.println("    **************** ****     ********    *");
		System.out.println("    **************** *    *   *           *");
		System.out.println("    *****       **** * * *    *           *");
		System.out.println("    *****       **** *    *   *           *");
		System.out.println("    *****       **** ****     ********    *");
		System.out.println("    ***************************************");
		
		

