package ba.BITCamp.Mladen.MojPaket;

public class MojaKlasa {

	public static void main(String[] args) {

		int h=4;
		int R=6;
		double r=R/2.0;
		
		double P=(2*r*r*Math.PI)+(2*r*Math.PI*h);
		double V=r*r*Math.PI*h;
		
		
		
		System.out.printf("Povrsina valjka iznosi:%f",P);
		System.out.printf("Zapremina valjka iznosi:%f",V);
	}

}
