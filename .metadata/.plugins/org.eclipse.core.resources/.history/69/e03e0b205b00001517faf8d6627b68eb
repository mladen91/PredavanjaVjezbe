package ba.BITCamp.Mladen.MojPaket;

public class Vjezbe {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int br1=-5;
		boolean pozitivan = br1>=0;
		
		
		if (br1>=0) {
			System.out.println("Broj je pozitivan " +pozitivan);	
		}
		else if (br1<0) {
			
			System.out.println("Broj je negativan"+pozitivan);
			
		}
        	
		
		
		
	}

}
